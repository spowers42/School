function [ f1, f2, bestSet ] = ransac_translation( f1, f2, matches )
%RANSAC_TRANSLATION Summary of this function goes here
%   Detailed explanation goes here
    N = 10;
    bestSet = [];
    bestOffset = [];
    for idx = randsample(size(matches, 2), N)
        m = matches(:, idx);
        offset = [f1(1, m(1)) - f2(1, m(2)), f1(2, m(1))-f2(2, m(2))];
        currentSet = [];
        for i = 1:size(matches,2)
            pair = matches(:, i);
            currentoffset = [f1(1, pair(1)) - f2(1, pair(2)), f1(2, pair(1))-f2(2, pair(2))];
            distance = sqrt(sum((offset-currentoffset).^2));
            if distance <10
                currentSet = [currentSet, pair];
            end
        end
        if size(currentSet,2)>size(bestSet,2) 
            bestSet = currentSet;
            best
        end
    end
end

